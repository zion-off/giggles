{
  "name": "documentation",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "start": "next start",
    "types:check": "fumadocs-mdx && next typegen && tsc --noEmit",
    "postinstall": "fumadocs-mdx",
    "lint": "prettier --write --loglevel warn . && eslint . --fix --quiet",
    "generate-og": "tsx scripts/generate-og.ts",
    "preview": "tsx scripts/generate-og.ts && opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "tsx scripts/generate-og.ts && opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@opennextjs/cloudflare": "^1.16.5",
    "@orama/orama": "^3.1.18",
    "fumadocs-core": "16.6.1",
    "fumadocs-mdx": "14.2.7",
    "fumadocs-ui": "16.6.1",
    "giggles": "workspace:*",
    "ink": "^6.8.0",
    "ink-web": "^0.1.11",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-reconciler": "^0.33.0",
    "scheduler": "^0.27.0",
    "tailwind-merge": "^3.4.0",
    "xterm": "^5.3.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/mdx": "^2.0.13",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.0.0",
    "eslint-config-next": "16.1.6",
    "postcss": "^8.5.6",
    "prettier": "^2.8.7",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "tsx": "^4.21.0",
    "wrangler": "^4.67.0"
  }
}
